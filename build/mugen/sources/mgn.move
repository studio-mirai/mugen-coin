module mugen::mgn;

use sui::coin::{Self, Coin, TreasuryCap};
use sui::url;

public struct MGN has drop {}

fun init(otw: MGN, ctx: &mut TxContext) {
    let (treasury_cap, coin_metadata) = coin::create_currency(
        otw,
        0,
        b"MGN",
        b"無限",
        b"To infinity and beyond.",
        option::some(
            url::new_unsafe_from_bytes(
                b"data:image/png;base64,",
            ),
        ),
        ctx,
    );

    transfer::public_share_object(treasury_cap);
    transfer::public_freeze_object(coin_metadata);
}

public fun mint(cap: &mut TreasuryCap<MGN>, amount: u64, ctx: &mut TxContext): Coin<MGN> {
    cap.mint(amount, ctx)
}

public fun burn(cap: &mut TreasuryCap<MGN>, coin: Coin<MGN>) {
    cap.burn(coin);
}
